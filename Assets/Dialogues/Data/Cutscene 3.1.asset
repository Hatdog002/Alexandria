  scenes:
  - sceneName: Scene 1
    dialogueLines:
    - speakerName: Narrator
      dialogueText: "The survivor wakes up in a tent near the leader\u2019s house."
      backgroundImage: {fileID: 21300000, guid: f3cb7987734561741a41008514887ed1, type: 3}
      triggerCameraShake: 0
    - speakerName: Narrator
      dialogueText: "The settlers have already begun to speak but only using the
        words in the children\u2019s alphabet along with basic words."
      backgroundImage: {fileID: 21300000, guid: fa2837997e5539a49a5359167b2368e4, type: 3}
      triggerCameraShake: 0
    - speakerName: Narrator
      dialogueText: The survivor heads inside the leader's house.
      backgroundImage: {fileID: 0}
      triggerCameraShake: 0
    - speakerName: Narrator
      dialogueText: The survivor entered the house and speak with the leader.
      backgroundImage: {fileID: 21300000, guid: f3cb7987734561741a41008514887ed1, type: 3}
      triggerCameraShake: 0
  - sceneName: Scene 2
    dialogueLines:
    - speakerName: Survivor
      dialogueText: "Uh\u2026 Hello sir?"
      backgroundImage: {fileID: 21300000, guid: 265ed396054880b4caee043642961e16, type: 3}
      triggerCameraShake: 0
    - speakerName: Survivor
      dialogueText: Alright, have you considered my offer yet?
      backgroundImage: {fileID: 0}
      triggerCameraShake: 0
    - speakerName: Leader
      dialogueText: "Hmm\u2026 Person, book, talk."
      backgroundImage: {fileID: 21300000, guid: b0f901adb3449c54da846ed2717462d8, type: 3}
      triggerCameraShake: 0
    - speakerName: Narrator
      dialogueText: The survivor is confused at first, but he thinks that he understands
        the leader.
      backgroundImage: {fileID: 0}
      triggerCameraShake: 0
    - speakerName: Survivor
      dialogueText: "Right\u2026 I think I get what you\u2019re trying to say."
      backgroundImage: {fileID: 21300000, guid: 265ed396054880b4caee043642961e16, type: 3}
      triggerCameraShake: 0
    - speakerName: Narrator
      dialogueText: 'Before the leader could respond the survivor leaves. '
      backgroundImage: {fileID: 21300000, guid: f3cb7987734561741a41008514887ed1, type: 3}
      triggerCameraShake: 0
  - sceneName: Scene 3
    dialogueLines:
    - speakerName: Narrator
      dialogueText: The survivor walks out of the settlement to find another books.
      backgroundImage: {fileID: 21300000, guid: 30a10803ef56ac449887d219bb868dbd, type: 3}
      triggerCameraShake: 0
